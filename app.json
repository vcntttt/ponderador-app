{
  "expo": {
    "name": "Ponderador",
    "slug": "ponderador",
    "version": "0.5.1",
    "orientation": "portrait",
    "icon": "./assets/images/tools.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/tools.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.vriveradev.ponderador"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/tools.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/tools.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "76c1e08c-40df-430a-9b08-925f93e08024"
      }
    }
  }
}
